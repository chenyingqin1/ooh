@font-face {
	font-family: uniicons;
	font-weight: normal;
	font-style: normal;
	src: url(data:font/ttf;base64,) format('truetype');
}
    /*通用 */
view,
	scroll-view,
	swiper,
	swiper-item,
	cover-view,
	cover-image,
	icon,
	text,
	rich-text,
	progress,
	button,
	checkbox,
	form,
	input,
	label,
	radio,
	slider,
	switch,
	textarea,
	navigator,
	audio,
	camera,
	image,
	video {
		line-height: 1;
		box-sizing: border-box;
		-webkit-flex-shrink: 0;
		        flex-shrink: 0;
}
view{
		font-family: '黑体','sans-serif';
		box-sizing: border-box;
}
image{
		height:auto;
}
