/* 清除浮动 */
.clearfloat:after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0;
}
.clearfloat {
  zoom: 1;
}
.flex-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.flex-col {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.flex-one {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.flex-two {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
}
.flex-three {
  -webkit-box-flex: 3;
